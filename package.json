{
  "name": "fastify-starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "yarn build && node dist/main",
    "dev": "yarn env:dev && nodemon | pino-pretty -c",
    "env:dev": "cross-env NODE_ENV=development",
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.6.1",
    "cross-env": "^7.0.3",
    "fastify": "^3.23.1",
    "fastify-compress": "^3.6.1",
    "fastify-cors": "^6.0.2",
    "fastify-decorators": "^3.10.0",
    "fastify-env": "^2.1.1",
    "fastify-multer": "^2.0.2",
    "fastify-no-icon": "^4.0.0",
    "fastify-rate-limit": "^5.6.2",
    "fastify-routes-stats": "^2.0.1",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/http-errors": "^1.8.1",
    "@types/node": "^16.0.0",
    "nodemon": "^2.0.14",
    "pino-pretty": "^7.2.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
